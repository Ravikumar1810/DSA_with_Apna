import java.util.*; 
public class GenericCollectionExample { 
    public static void main(String[] args) { 

    List<String> names = new ArrayList<>(); 

   names.add("John"); 
   names.add("Alice"); 
   names.add("Bob"); 

    System.out.println("Names List:"); 
     for (String name : names) { 
      System.out.println(name); 
    } 

  } 
}
